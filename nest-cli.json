{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true
  },
  "projects": {
    "tattoo-book-architecture": {
      "type": "library",
      "root": "libs/tattoo-book-architecture",
      "entryFile": "index",
      "sourceRoot": "libs/tattoo-book-architecture/src",
      "compilerOptions": {
        "tsConfigPath": "libs/tattoo-book-architecture/tsconfig.lib.json"
      }
    }
  }
}