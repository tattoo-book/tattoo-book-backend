{
  "name": "tattoo-book-backend",
  "version": "1.0.0",
  "description": "Projeto da discipline Engenharia de Software",
  "author": "Diogo Rodrigues",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "dotenv -- nest start --watch",
    "build": "nest build",
    "docker-build": "docker build -t tattoo-book-backend --file ./deploy/dev/Dockerfile .",
    "docker-run": "docker run -d --rm --env-file .env --name tattoo-book-backend  --network host tattoo-book-backend",
    "docker-run-database": "docker run -d  --env-file=.env --name tattoo-book-database --hostname tattoo-book-database -p 5432:5432 postgres",
    "docker-connect-network-container": "docker network connect tattoo-book-network tattoo-book-database; docker network connect tattoo-book-network tattoo-book-backend",
    "docker-create-network": "docker network create tattoo-book-network",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --verbose --config ./jest.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@google-cloud/vision": "^4.3.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^11.0.11",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/typeorm": "^10.0.2",
    "@types/bcrypt": "^5.0.2",
    "@types/multer": "^1.4.12",
    "@nestjs/cli": "^10.0.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "exceljs": "^4.4.0",
    "joi": "^17.13.3",
    "module-alias": "^2.2.3",
    "multer": "^1.4.5-lts.1",
    "nestjs-i18n": "^10.5.0",
    "nestjs-joi": "^1.10.1",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20",
    "ts-loader": "^9.4.3"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^11.1.5",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv": "^17.2.0",
    "dotenv-cli": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  }
}